{
  "env": {
    "development": {
      "presets": ["next/babel"],
      "plugins": [
        "@babel/plugin-proposal-export-namespace-from",
        ["styled-components", { "ssr": true, "displayName": true, "preprocess": false } ]
      ]
    },
    "production": {
      "presets": ["next/babel"],
      "plugins": [
        "@babel/plugin-proposal-export-namespace-from",
        ["styled-components", { "ssr": true, "displayName": false, "preprocess": false } ]
      ]
    },
  }
}
